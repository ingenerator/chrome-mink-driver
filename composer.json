{
    "name": "ingenerator/chrome-mink-driver",
    "description": "Mink driver for controlling chrome without selenium",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Dorian More",
            "email": "doriancmore@gmail.com"
        },
        {
            "name": "Craig Gosman",
            "email": "craig@ingenerator.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "DMore\\ChromeDriver\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DMore\\ChromeDriverTests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "replace": {
        "dmore/chrome-mink-driver": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "mink/driver-testsuite": "dev-master",
        "dms/phpunit-arraysubset-asserts": "^0.4.0"
    },
    "require": {
        "ext-curl": "*",
        "ext-json": "*",
        "textalk/websocket": "^1.5.0",
        "behat/mink": "^1.7"
    }
}
